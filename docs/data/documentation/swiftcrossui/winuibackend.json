      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "WinUIBackend supports both ARM64 and x64 Windows 10\/11 computers. It is the recommended backend to use when compiling SwiftCrossUI apps for Windows, as it aims to provide the most native experience.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "System-dependencies",
          "level" : 2,
          "text" : "System dependencies",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Before you can use WinUIBackend you must install two dependencies; the former is only required at compile time while the later is only required at runtime.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Install Windows SDK 10.0.17736;",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                },
                {
                  "code" : [
                    "winget install --id Microsoft.WindowsSDK.10.0.17736"
                  ],
                  "syntax" : "sh",
                  "type" : "codeListing"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Install the WindowsAppSDK 1.5.240205001-preview1 variant for your architecture; ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "https:\/\/aka.ms\/windowsappsdk\/1.5\/1.5.240205001-preview1\/windowsappruntimeinstall-x64.exe",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : "\/",
                      "type" : "text"
                    },
                    {
                      "identifier" : "https:\/\/aka.ms\/windowsappsdk\/1.5\/1.5.240205001-preview1\/windowsappruntimeinstall-arm64.exe",
                      "isActive" : true,
                      "type" : "reference"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
